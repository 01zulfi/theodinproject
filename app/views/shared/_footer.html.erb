<footer id="footer">
  <div class="footer-logo">
    <%= render "shared/logo_text" %>
  </div>
  <div class="footer-social">
    <%= link_to image_tag("icons/icon-github.svg"), "https://github.com/TheOdinProject"%>
    <%= link_to image_tag("icons/icon-gitter.svg"), "https://gitter.im/TheOdinProject"%>
    <%= link_to image_tag("icons/icon-facebook.svg"), "https://www.facebook.com/theodinproject/"%>
    <%= link_to image_tag("icons/icon-twitter.svg"), "https://twitter.com/TheOdinProject"%>
  </div>
  <div class="footer-links">
    <div class="footer-links-container">
      <%= link_to "About", about_path %>
      <%= link_to "FAQ", faq_path %>
      <%= link_to "Blog", medium_blog_path, target: "_blank" %>
    </div>
    <div class="footer-links-container">
      <%= link_to "Success Stories", success_stories_path %>
      <%= link_to "Contribute", contributing_path %>
    </div>
  </div>


</footer>
